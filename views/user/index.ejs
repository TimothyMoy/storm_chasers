<!DOCTYPE html>
<html lang="en">
<!-- Head -->
<%- include('../partials/head2', {title: ' - All Users'}) %>
<body id="adminPage">
<!-- Header -->
    <%- include('../partials/dashHeader') %>
    <main class ="container">
        <label for="dashboard" id="dashLabel">Dashboard</label>
        <div class="mainBox">
            <div class="innerbox1 row">
                <h1>Members</h1>
            <% users.forEach((user) => { %>
          
                 <li id="usersList"><a href="/admin/<%= user.id%>"><%= user.firstName + " " + user.lastName + " " + user.email + " created: " + user.createdAt%></a></li>
            <% }); %> 
            </div>
        </div>
    </main>
<!-- Footer -->
    <%- include('../partials/footer') %>
</body>
</html>