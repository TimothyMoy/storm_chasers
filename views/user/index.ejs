<!DOCTYPE html>
<html lang="en">
<!-- Head -->
<%- include('../partials/head', {title: ' - All Users'}) %>
<body id="">
<!-- Header -->
    <%- include('../partials/header') %>
    <main>
        <div>
            <h1>Users</h1>
        <% users.forEach((user) => { %>
          
              <h4><a href="/admin/<%= user.id%>"><%= user.firstName + " " + user.lastName + " " + user.email%></a></h4>
      <% }); %>
        </div>
    </main>
<!-- Footer -->
    <%- include('../partials/footer') %>
</body>
</html>